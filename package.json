    "postcss-import": "^8.0.2",
    "postcss-loader": "^2.1.3",
    "precss": "^3.1.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "regenerator-runtime": "^0.11.1",
    "replace-in-file": "^3.4.0",
    "style-loader": "^0.20.3",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1",
    "xhr-mock": "^2.3.2"
  }
}
